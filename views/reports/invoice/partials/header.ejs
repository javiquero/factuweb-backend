<div class="header">
	<% include ./letterhead %>

		<div class="invoice-head">
			<div class="row">
				<div class="col-7 " style="padding-top:30px;">
					<div class="row">
						<div class="col-12 text-center">
							Cliente número:
							<%= invoice.CLIFAC %>
						</div>
					</div>
					<div class="row table-head">
						<div class="col-3">Nº FACTURA</div>
						<div class="col-3">FECHA FACTURA</div>
						<div class="col-3">REPRESENTANTE</div>
						<div class="col-3">C.I.F o D.N.I</div>
					</div>
					<div class="row table-body">
						<div class="col-3">
							<%= invoice.TIPFAC %>
								<span v-if="invoice.type=='factusol'">-</span>
								<%= invoice.CODFAC %>
						</div>
						<div class="col-3">
							<%= InvoiceDate %>
						</div>
						<div class="col-3">
							<%= invoice.AGEFAC %>
						</div>
						<div class="col-3">
							<%= invoice.CNIFAC %>
						</div>
					</div>
				</div>
				<div class="col-5 text-left info-client">
					<%= invoice.CNOFAC %>
						<br>
						<%= invoice.NOCCLI %>
							<br>
							<%= invoice.CDOFAC %>
								<br>
								<%= invoice.CPOFAC + " " + invoice.CCPFAC %>
									<br>
									<%= invoice.CPRFAC %>
				</div>
			</div>
		</div>

</div>
