<div class="body">
	<div class="lines">
		<div class="lines-head">
			<div class="row">
				<div class="col-small">
					CODIGO
				</div>
				<div class="col-large">
					DESCRIPCION
				</div>
				<div class="col-small">
					CANTIDAD
				</div>
				<div class="col-small">
					PRECIO
				</div>
				<div class="col-small">
					TOTAL
				</div>
			</div>
		</div>
		<div class="lines-body">
			<%
				var i = 0;
				for (i = 0; i <= linesXpage-1 && (p * linesXpage) + i < invoice.lines.length; i++ ){
				let index = (p * linesXpage) + i;
				let line = invoice.lines[index];
			%>
				<div class="row line">
					<div class="col-small text-right" style="padding-left:20px;">
						<%= line.ARTLFA %>
					</div>
					<div class="col-large">
						<%= line.DESLFA %>
					</div>
					<div class="col-small text-center">
						<%= line.CANLFA!=0?line.CANLFA:'' %>

					</div>
					<div class="col-small text-right">
						<% let PRELFA = (line.PRELFA / 1).toFixed(2).toString(); %>
							<%= PRELFA != 0 ? PRELFA.replace(".",",") : '' %>
								<% if (line.DT1LFA>0){%>
									<span style="color:green">
										<%=' -' + line.DT1LFA +' %'%>
									</span>
									<%}%>
					</div>
					<div class="col-small text-right">
						<% let totalLinea = line.TOTLFA; %>
							<% totalLinea = (totalLinea / 1).toFixed(2).toString(); %>
								<%=totalLinea!=0?totalLinea.replace(".",","):'' %>
					</div>
				</div>
				<% }
				for (; i < linesXpage+3; i++ ){%>
					<div class="row line">
						<div class="col-small" style="padding-left:20px;">
							&nbsp;
						</div>
						<div class="col-large">
							&nbsp;
						</div>
						<div class="col-small text-center">
							&nbsp;
						</div>
						<div class="col-small text-right">
							&nbsp;
						</div>
						<div class="col-small text-right">
							&nbsp;
						</div>
					</div>
					<%	}
				%>
		</div>
		<div class=" lines-footer">
			<div class="row">
				<div class="col-12">
					Registro Mercantil de Barcelona, Tomo 7019, Libro 6294, Sección.2ª, Folio 29, Hoja 84106, Inscripción.1ª, Fecha 7/3/86 N.I.F.
					A-58091489
				</div>
			</div>
		</div>
	</div>
</div>
